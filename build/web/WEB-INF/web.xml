<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DownloadImage</servlet-name>
        <servlet-class>com.pst.downloader.DownloadImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processRegisterActivity</servlet-name>
        <servlet-class>processRegisterActivity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contoh</servlet-name>
        <servlet-class>contoh</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processStudentPage</servlet-name>
        <servlet-class>processStudentPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processEditStudent</servlet-name>
        <servlet-class>processEditStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processPending</servlet-name>
        <servlet-class>processPending</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>acceptPending</servlet-name>
        <servlet-class>acceptPending</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>accept</servlet-name>
        <servlet-class>accept</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadImage</servlet-name>
        <url-pattern>/DownloadImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processRegisterActivity</servlet-name>
        <url-pattern>/processRegisterActivity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contoh</servlet-name>
        <url-pattern>/contoh</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processStudentPage</servlet-name>
        <url-pattern>/processStudentPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processEditStudent</servlet-name>
        <url-pattern>/processEditStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processPending</servlet-name>
        <url-pattern>/processPending</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acceptPending</servlet-name>
        <url-pattern>/acceptPending</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>accept</servlet-name>
        <url-pattern>/accept</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
